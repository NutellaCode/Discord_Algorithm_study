package study.week3;

import java.util.Scanner;

public class q10870 {
	private static int fibo(int n) {
		if(n<=2)return 1;
		
		return fibo(n-1)+fibo(n-2);
	}

	public static void main(String[] args) {

		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		fibo(n);
		System.out.println(fibo(n));
	}
}
